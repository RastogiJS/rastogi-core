{
  "name": "rastogi-nsp",
  "version": "0.0.1",
  "description": "Module for fetching NSP advisories, and keeping historical record over time",
  "main": "index.js",
  "scripts": {
    "test": "npm run unit",
    "pretest": "npm run format && npm run lint",
    "commit": "commit-wizard",
    "ban": "ban",
    "unit": "tape \"src/**/*.spec.js\"",
    "lint": "standard --verbose \"src/**/*.js\"",
    "format": "standard-format -w ",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "license": "license-checker --production --onlyunknown --csv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RastogiJS/rastogi-core.git"
  },
  "keywords": [
    "nsp",
    "rastogijs",
    "security",
    "dependencies"
  ],
  "author": "Joseph Hejderup <j.i.hejderup@tudelft.nl> (https://twitter.com/jhejderup)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RastogiJS/rastogi-core/issues"
  },
  "homepage": "https://github.com/RastogiJS/rastogi-core#readme",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run test",
        "npm run ban"
      ],
      "pre-push": [
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "devDependencies": {
    "ban-sensitive-files": "1.8.3",
    "license-checker": "5.1.2",
    "nock": "8.0.0",
    "nyc": "6.6.1",
    "pre-git": "3.10.0",
    "semantic-release": "4.3.5",
    "standard": "7.1.2",
    "standard-format": "2.2.2",
    "tape": "4.6.0"
  },
  "dependencies": {
    "bee-queue": "0.3.0",
    "config": "1.21.0",
    "eventsource": "0.2.1",
    "jsondiffpatch": "0.2.4",
    "memoizee": "0.4.1",
    "rethinkdb": "2.3.3",
    "rxjs": "5.0.0-beta.10",
    "semver": "5.3.0"
  }
}
